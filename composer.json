{
    "name": "supermetrolog/synchronizer-already-sync-repo",
    "description": "Already synchronized repository interface implemetation",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "composer phpunit & composer phpstan & composer phpcs",
        "phpunit": "./vendor/bin/phpunit",
        "phpstan": "./vendor/bin/phpstan analyse --xdebug",
        "phpcs": "./vendor/bin/phpcs -p -w",
        "phpcbf": "./vendor/bin/phpcbf -p -w"
    },
    "autoload": {
        "psr-4": {
            "Supermetrolog\\SynchronizerAlreadySyncRepo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require": {
        "supermetrolog/synchronizer": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "*"
    },
    "authors": [{
        "name": "supermetrolog",
        "email": "billypro6@gmail.com"
    }],
    "minimum-stability": "stable"
}